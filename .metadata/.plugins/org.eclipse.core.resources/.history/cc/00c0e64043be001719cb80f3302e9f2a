package p1;

public class Demo {

	public static void main(String[] args) {
		String[] names = new String[10];
		names[0] = "Adam";
		names[1] = "Bill";
		for(int i = 0; i < 2; i++) {
			System.out.println(names[i]);
			System.out.println(names[i].length());
		}
		System.out.println("Done");
				
	}

}
